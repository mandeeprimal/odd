package com.leapfrog.sum;

/**
 *
 * @author itcomputer
 */
public class java {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int p=0,c=0;
        for(int i=2;i<=2;i++){
            for(int j=1;j<=10;j++){
                if(j%2!=0){
                    p=(i*j);
                System.out.println(p);
                c+= p;
            }
            
        }
        
        // TODO code application logic here
    }
    System.out.println(c);
}
